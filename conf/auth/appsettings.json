{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "System": "Warning"
    },
    "Console": {
      "FormatterName": "Simple",
      "FormatterOptions": {
        "UseUtcTimestamp": true,
        "IncludeScopes": true,
        "ColorBehavior": "Enabled"
      }
    }
  },
  "Mongo": {
    "ConnectionString": "mongodb://my_username:my_password@auth-storage:27017"
  },
  "AllowedHosts": "*",
  "W3C": {
    "LogDirectory": "/logs/",
    "FileName": "w3c.log"
  },
  "RabbitMQ": {
    "Port": "5672",
    "HostName": "queue",
    "UserName": "myUser",
    "Password": "myPassword"
  }
}
