{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Conduit.Auth.ApplicationLayer": "Warning",
      "Microsoft": "Warning",
      "System": "Warning"
    },
    "Console": {
      "FormatterName": "Simple",
      "FormatterOptions": {
        "UseUtcTimestamp": true,
        "IncludeScopes": true,
        "ColorBehavior": "Enabled"
      }
    }
  },
  "Dapper": {
    "ConnectionOptions": {
      "ConnectionString": "Server=auth-storage;Port=5432;Database=postgres;User Id=postgres;Password=my_password;"
    }
  },
  "AllowedHosts": "*",
  "W3C": {
    "LogDirectory": "/logs/",
    "FileName": "w3c.log"
  },
  "RabbitMQ": {
    "Port": "5672",
    "HostName": "queue",
    "UserName": "myUser",
    "Password": "myPassword"
  }
}
