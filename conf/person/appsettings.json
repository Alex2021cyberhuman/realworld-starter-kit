{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "System": "Warning"
    },
    "Console": {
      "FormatterName": "Simple",
      "FormatterOptions": {
        "UseUtcTimestamp": true,
        "IncludeScopes": true,
        "ColorBehavior": "Enabled"
      }
    }
  },
  "AllowedHosts": "*",
  "W3C": {
    "LogDirectory": "/logs/",
    "FileName": "w3c.log"
  },
  "RabbitMQ": {
    "Port": "5672",
    "HostName": "queue",
    "UserName": "myUser",
    "Password": "myPassword"
  },
  "Neo4J": {
    "Password": "my_password",
    "Username": "neo4j",
    "Uri": "bolt://person-storage:7687"
  },
  "Events": {
    "RegisterUser": {
      "Queue": "queue-registerusereventmodel-person"
    },
    "UpdateUser": {
      "Queue": "queue-updateusereventmodel-person"
    }
  }
}
