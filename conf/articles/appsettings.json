{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "System": "Warning"
    },
    "Console": {
      "FormatterName": "Simple",
      "FormatterOptions": {
        "UseUtcTimestamp": true,
        "IncludeScopes": true,
        "ColorBehavior": "Enabled"
      }
    }
  },
  "ConnectionStrings": {
    "Articles": "Host=articles-storage; Database=articles_database; Username=my_username; Password=my_password; Include Error Detail=true; Log Parameters=true;"
  },
  "AllowedHosts": "*",
  "W3C": {
    "LogDirectory": "/logs/",
    "FileName": "w3c.log",
    "LoggingFields": "All"
  },
  "RabbitMQ": {
    "Port": "5672",
    "HostName": "queue",
    "UserName": "myUser",
    "Password": "myPassword"
  }
}
